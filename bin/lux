#!/usr/bin/env node
'use strict';

const PWD = process.env.PWD;

try {
  const Lux = require(`${PWD}/node_modules/lux-framework`);

  if (typeof Lux.CLI === 'function') {
    Lux.CLI();
  } else {
    require(`${PWD}/node_modules/lux-framework/dist/packages/cli`);
  }
} catch (err) {
  require('../dist').CLI();
}
